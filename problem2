	switch(letter){
		case 'c':
		cout<<"YES\n";
		break;
		case 'o':
		cout<<"YES\n";
		break;
		case 'd':
		cout<<"yse\n";
		break;
		case 'e':
		cout<<"YES\n";
		break;
		case 'f':
		cout<<"YES\n";
		break;
		case 'r':
		cout<<"YES\n";
		break;
		case 's':
		cout<<"YES\n";
		break;
		default:
		cout<<" NO\n";
		
	}
	
